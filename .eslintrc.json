{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "xo",
	"overrides": [
		{
			"extends": [
				"xo-typescript"
			],
			"files": [
				"*.ts",
				"*.tsx"
			],
			"rules": {
				"@typescript-eslint/naming-convention": [
					0, {
						"selector": "typeLike",
						"format": ["StrictPascalCase"]
					}
				],
				"@typescript-eslint/consistent-type-definitions": 0
			}
		}
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-console": 1,
		"no-import-assign": 2
	}
}