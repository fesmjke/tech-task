{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "xo",
	"overrides": [
		{
			"extends": [
				"xo-typescript"
			],
			"files": [
				"*.ts",
				"*.tsx"
			]
		}
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-console": 1,
		"no-import-assign": 2
	}
}